%% PAPELES DISTRIBUCIÓN
t = []; %Llenar con datos 
to = sort(t);
n = length(to);
I = 0.5:1:n -0.5;
E = I/n; %Empírica
F = E;

%%Exponencial
y = log(1-F);
plot(to, y, 'o')
fitlm(to, y)

%%Weibull
y = log(-log(1-F));
x = log(to);
plot(x,y,'o')
fitlm(x ,y)

%%Valor extremo
y=log(-log(1-F));
plot(to,y,'o')
fitlm(to,y)

%%Normal
y = norminv(F);
plot(to, y, 'o')
fitlm(to, y)

%%Lognormal
y = norminv(F);
x = log(to);
plot(x, y, 'o')
fitlm(x,y)
.gitattributes
